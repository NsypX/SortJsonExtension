{
  "name": "json-markup-sort",
  "displayName": "JsonMarkupSort",
  "description": "sort a marked-up json",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json-markup-sort.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "json-markup-sort.sortJsonKeys",
        "title": "Sort JSON Keys Alphabetically"
      }
    ],
    "keybindings": [
      {
        "command": "json-markup-sort.sortJsonKeys",
        "key": "f7",
        "mac": "F7",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "test:jest": "jest --runInBand --coverage --silent --detectOpenHandles"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  }
}
